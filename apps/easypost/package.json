{
  "name": "@mcp-shipping/easypost-mcp-server",
  "version": "1.0.0",
  "description": "Production-ready EasyPost MCP Server implementing Model Context Protocol for shipping automation",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "easypost-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "tsx --watch src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist coverage",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:e2e": "jest --config jest.e2e.config.js --passWithNoTests",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write src tests",
    "format:check": "prettier --check src tests",
    "docker:build": "docker build -t easypost-mcp .",
    "docker:run": "docker run -p 3000:3000 --env-file .env easypost-mcp",
    "precommit": "npm run lint && npm run typecheck && npm run test",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "zod": "^3.24.1",
    "winston": "^3.17.0",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "fast-json-stable-stringify": "^2.1.0",
    "node-cache": "^5.1.2",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "prettier": "^3.4.2",
    "nock": "^13.5.6",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "easypost",
    "shipping",
    "api",
    "json-rpc",
    "typescript",
    "docker"
  ],
  "author": "MCP Shipping Suite Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcp-shipping/easypost-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/mcp-shipping/easypost-mcp-server/issues"
  },
  "homepage": "https://github.com/mcp-shipping/easypost-mcp-server#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}