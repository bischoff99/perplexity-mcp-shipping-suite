{
  "name": "@mcp-shipping/shared",
  "version": "1.0.0",
  "description": "Shared utilities and types for MCP shipping automation platform",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "typescript": "~5.9.2"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "mcp",
    "shipping",
    "shared",
    "utilities",
    "types"
  ],
  "author": "MCP Shipping Suite Team",
  "license": "MIT"
}
