{
  "name": "@mcp-shipping/veeqo-mcp-server",
  "version": "1.0.0",
  "description": "Production-ready Veeqo MCP Server implementing Model Context Protocol for inventory management and order automation",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "veeqo-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "tsx --watch src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist coverage",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:e2e": "jest --config jest.e2e.config.js --passWithNoTests",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write src tests",
    "format:check": "prettier --check src tests",
    "docker:build": "docker build -t veeqo-mcp .",
    "docker:run": "docker run -p 3000:3000 --env-file .env veeqo-mcp",
    "precommit": "npm run lint && npm run typecheck && npm run test",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "webhook:dev": "npm run dev -- --webhook-port=3001"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "axios": "^1.12.1",
    "bottleneck": "^2.19.5",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "fast-json-stable-stringify": "^2.1.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.7.0",
    "node-cache": "^5.1.2",
    "winston": "^3.17.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.3.3",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^9.1.2",
    "jest": "^29.7.0",
    "nock": "^13.5.6",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "ws": "^8.18.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "veeqo",
    "inventory",
    "ecommerce",
    "api",
    "json-rpc",
    "typescript",
    "docker",
    "orders",
    "products",
    "warehouse",
    "webhooks"
  ],
  "author": "MCP Shipping Suite Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcp-shipping/veeqo-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/mcp-shipping/veeqo-mcp-server/issues"
  },
  "homepage": "https://github.com/mcp-shipping/veeqo-mcp-server#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
